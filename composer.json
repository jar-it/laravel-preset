    "license": "MIT",
    "require": {
        "laravel/framework": "^5.7 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "JarIt\\LaravelPreset\\": "src/"
        }
    },
    "extra": {
        "laravel": {
